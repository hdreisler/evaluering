<h1>Listing customercompanies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Inactive</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @customercompanies.each do |customercompany| %>
  <tr>
    <td><%= customercompany.name %></td>
    <td><%= customercompany.inactive %></td>
    <td><%= link_to 'Show', customercompany %></td>
    <td><%= link_to 'Edit', edit_customercompany_path(customercompany) %></td>
    <td><%= link_to 'Destroy', customercompany, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

